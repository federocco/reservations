{
  "name": "reservations",
  "version": "1.0.0",
  "description": "Reservations",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && node_modules/typescript/bin/tsc",
    "migrate": "npm run build && node_modules/.bin/sequelize db:migrate --env development",
    "migrate:undo": "npm run build && node_modules/.bin/sequelize db:migrate:undo --env development",
    "migrate:undo:all": "npm run build && node_modules/.bin/sequelize db:migrate:undo:all --env development",
    "dev": "nodemon",
    "postinstall": "node-config-ts"
  },
  "keywords": [
    "reservations",
    "queue manager",
    "shop reservations"
  ],
  "author": {
    "url": "http://www.zoroaster.it",
    "name": "Federico Zoroaster"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/federocco/reservations.git"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 13.13.0"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "config": "^3.2.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql2": "^2.1.0",
    "node-config-ts": "^3.0.4",
    "query-string": "^6.12.1",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.5",
    "@types/sequelize": "^4.28.6",
    "@types/validator": "^13.0.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.1",
    "tslib": "^1.10.0",
    "typescript": "^3.7.2"
  }
}
